{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "tslint -c ./tslint.json 'src/**.{ts, tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint && yarn test",
      "pre-commit": "lint-staged -c ./lintstaged.json"
    }
  },
  "dependencies": {
    "babel-preset-gatsby": "^0.4.8",
    "gatsby": "^2.23.1",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^2.4.6",
    "gatsby-link": "^2.4.5",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-feed": "^2.5.4",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-offline": "^3.2.8",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-plugin-sitemap": "^2.4.4",
    "gatsby-plugin-styled-components": "^3.3.3",
    "gatsby-plugin-twitter": "^2.3.3",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-javascript-frontmatter": "^2.3.4",
    "gatsby-transformer-json": "^2.4.4",
    "gatsby-transformer-remark": "^2.8.14",
    "gatsby-transformer-sharp": "^2.5.4",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-form-with-constraints": "^0.15.0",
    "react-helmet": "^6.0.0",
    "react-use-scroll-position": "^2.0.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.2.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/lunr": "^2.3.3",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-typescript": "^2.4.4",
    "gatsby-remark-copy-linked-files": "^2.3.4",
    "gatsby-remark-images": "^3.3.9",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "lodash": "^4.17.11",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.5"
  }
}
