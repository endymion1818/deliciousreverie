{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "tslint -c ./tslint.json 'src/**.{ts, tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint && yarn test",
      "pre-commit": "lint-staged -c ./lintstaged.json"
    }
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "^3.3.1",
    "babel-preset-gatsby": "^1.13.0",
    "gatsby": "^3.13.0",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^3.11.0",
    "gatsby-link": "^3.13.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-feed": "^3.13.0",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-no-javascript-utils": "^0.7.2",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-plugin-twitter": "^3.13.0",
    "gatsby-plugin-webpack-size": "^2.0.1",
    "gatsby-remark-highlight-code": "^3.0.1",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-javascript-frontmatter": "^3.13.0",
    "gatsby-transformer-json": "^3.13.0",
    "gatsby-transformer-remark": "^4.10.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-form-with-constraints": "^0.18.0",
    "react-helmet": "^6.0.0",
    "react-use-scroll-position": "^2.0.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.155",
    "@types/lunr": "^2.3.3",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-remark-copy-linked-files": "^4.10.0",
    "gatsby-remark-images": "^5.10.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.11",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  }
}
