{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "tslint -c ./tslint.json 'src/**.{ts, tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint && yarn test",
      "pre-commit": "lint-staged -c ./lintstaged.json"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "babel-preset-gatsby": "^0.2.26",
    "gatsby": "^2.18.17",
    "gatsby-awesome-pagination": "^0.3.4",
    "gatsby-image": "^2.2.37",
    "gatsby-link": "^2.2.27",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-tinacms": "^0.1.29",
    "gatsby-plugin-twitter": "^2.1.17",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-tinacms-git": "^0.4.3",
    "gatsby-tinacms-mdx": "^0.1.5",
    "gatsby-transformer-javascript-frontmatter": "^2.1.22",
    "gatsby-transformer-json": "^2.2.22",
    "gatsby-transformer-remark": "^2.6.45",
    "gatsby-transformer-sharp": "^2.3.9",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-form-with-constraints": "^0.14.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/lunr": "^2.3.2",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.4.1",
    "babel-jest": "^24.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "^3.1.39",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.11",
    "prettier": "^1.17.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.4"
  }
}
